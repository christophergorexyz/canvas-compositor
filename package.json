{
  "name": "canvas-compositor",
  "version": "3.0.2",
  "description": "a light and performant canvas compositing engine",
  "main": "src/index.mjs",
  "scripts": {
    "prepare": "npm run build",
    "clean": "rm -rf lib/* docs/* dist/*",
    "build": "jsdoc -r README.md src/ -d docs & babel src/ -d lib/ -D -s & (browserify src/index.mjs -s CanvasCompositor -d -t [ babelify ] | uglifyjs > dist/canvas-compositor.min.js)"
  },
  "keywords": [
    "2d",
    "canvas",
    "compositing"
  ],
  "author": "Christopher J Gore",
  "repository": {
    "type": "git",
    "url": "git@github.com:christophergorexyz/canvas-compositor.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
    "jsdoc": "^3.6.7",
    "uglify-js": "^3.14.2"
  },
  "dependencies": {
    "caniuse-lite": "^1.0.30001448",
    "vectorious": "^5.5.0"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  }
}
